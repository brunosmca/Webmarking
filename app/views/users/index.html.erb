
<h1>Welcome</h1>
<br/>
<% if logged_in? %>
 	<p>You are logged in as <%=h current_user.login %></p>
 	<p><%= link_to 'Logout', logout_path %></p>
<% else %>
 	<p><strong>You are currently not logged in.</strong></p>
 	<p>
   	<%= link_to 'Login', login_path %> or
   	<%= link_to 'Sign Up', signup_path %>
 	</p>
<% end %>

